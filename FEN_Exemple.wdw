              code : |1+
               
               // Ouvre le sélecteur de fichiers
               sFichier est une chaîne = fSélecteur(frepexe(), "contact_export.json", "Sélectionnez un fichier...", "Fichier JSON (*.json)" + TAB + "*.json", "*.json", fselCrée + fselExiste)
               // Si aucun fichier n'est sélectionné, ne rien faire
               si sFichier ~= "" alors retour
               
               // Lance l'export du fichier
               si HExporteJSON(Contact, sFichier) = faux ALORS
               	erreur("Impossible d'exporter les enregistrements du fichier contact : ", herreurinfo())
               	retour
               FIN
               
               // Propose à l'utilisateur d'ouvrir le fichier généré avec le lecteur par défaut
               si dialogue("Les enregistrements du fichier contact ont bien été exportés.[%RC%]Souhaitez-vous ouvrir le fichier JSON avec l'application associée par défaut ?", ["Ouvrir le fichier", "Ne pas ouvrir"], 1, 2) = 1 alors 
