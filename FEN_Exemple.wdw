            -
              code : |1+
               
               // Mémorise le nombre de contact dans le fichier HFSQL
               soit nNbEnregistrement = hnbenr(contact)
               
               // Importe des données d'un fichier JSON dans le fichier HFSQL "contact"
               si himportejson(Contact, frepexe()+[fsep]+"contact_a_importer.json") = faux ALORS
               	Erreur("Impossible d'importer le fichier JSON : ", herreurinfo())
               	retour
               FIN
               
               // Fin de traitement
               si nNbEnregistrement = hnbenr(contact) ALORS
               	toastaffiche("Import JSON terminé[%rc%]Aucun enregistrement importé", toastCourt, cvhaut, chCentre)
               SINON
               	toastaffiche("Import JSON terminé[%RC%][%val(HNbEnr(Contact)-nNbEnregistrement)%] enregistrement(s) importé(s)", toastCourt, cvhaut, chCentre)
               FIN
