              code : |1+
               // Définit la requête
               // La commande "JSON_EXISTS" permet de récupérer les enregistrements dont la rubrique JSON contient une donnée
               // Ici, la requête permet de récupérer les produits ayant la valeur "talon" dans les caractéristiques (donc, toutes les chaussures à talons)
               sRequêteSQL est une chaine = [
               SELECT
               produit.Reference,
               produit.LIbellé,
               Produit.Caractéristiques
               FROM
               produit
               WHERE
               JSON_EXISTS (Produit.Caractéristiques, '$.talon?(@ == true)')
               ]
               
               // Pour savoir si la rubrique contient une donnée ayant une valeur précise (ici, la donnée doit valoir 'true'), la syntaxe est :
               // JSON_EXISTS (Produit.Caractéristiques, '$.talon?(@ == true)')
               
               // Exécute la requête
               sdRequête est une source de données
               si HExécuteRequêteSQL(sdRequête, sRequêteSQL) = faux ALORS
               	Erreur("Impossible d'exécuter la requête SQL : ", herreurinfo())
               	retour
               FIN
               
               sRésultat est une chaine
               nLimite est un entier = 3 // Limite l'affichage à 3 résultats
               // Parcourt les résultats
               pour TOUT sdRequête 
               	sRésultat += [RC+RC] + "> Produit : [%sdRequête.Référence%], [%sdRequête.Libellé%][%RC%]JSON : [%sdRequête.Caractéristiques%]"
               	
               	// Si la limite est atteinte, stoppe le parcours
               	nLimite--
               	si nLimite <= 0 alors sortir
               FIN
               
               // Affiche le résultat
               info("Produits ayant ""talon"" dans leurs caractéristiques : [%HNbEnr(sdRequête)%] résultats", "3 premiers résultats : ", sRésultat)
